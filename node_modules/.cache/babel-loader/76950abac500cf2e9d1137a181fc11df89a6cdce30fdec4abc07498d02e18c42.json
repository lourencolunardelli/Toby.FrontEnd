: false\n    }\n  },\n  data() {\n    return {\n      notifications: this.$notifications.state\n    };\n  },\n  methods: {\n    removeNotification(timestamp) {\n      this.$notifications.removeNotification(timestamp);\n    }\n  },\n  created() {\n    this.$notifications.settings.overlap = this.overlap;\n  },\n  watch: {\n    overlap: function(newVal) {\n      this.$notifications.settings.overlap = newVal;\n    }\n  }\n};\n</script>\n<style lang=\"scss\">\n.notifications {\n  .list-move {\n    transition: transform 0.3s, opacity 0.4s;\n  }\n  .list-item {\n    display: inline-block;\n    margin-right: 10px;\n  }\n  .list-enter-active {\n    transition: transform 0.2s ease-in, opacity 0.4s ease-in;\n  }\n  .list-leave-active {\n    transition: transform 1s ease-out, opacity 0.4s ease-out;\n  }\n\n  .list-enter {\n    opacity: 0;\n    transform: scale(1.1);\n  }\n  .list-leave-to {\n    opacity: 0;\n    transform: scale(1.2, 0.7);\n  }\n}\n</style>\n"],"mappings":"AAeA,OAAAA,YAAA;AAEA;EACAC,UAAA;IACAD,YAAA,EAAAA;EACA;EACAE,KAAA;IACAC,cAAA;MACAC,IAAA,EAAAC,MAAA;MACAC,OAAA;IACA;IACAC,cAAA;MACAH,IAAA,EAAAC,MAAA;MACAC,OAAA;IACA;IACAE,OAAA;MACAJ,IAAA,EAAAK,OAAA;MACAH,OAAA;IACA;EACA;EACAI,IAAA,WAAAA,KAAA;IACA;MACAC,aAAA,OAAAC,cAAA,CAAAC;IACA;EACA;EACAC,OAAA;IACAC,kBAAA,WAAAA,mBAAAC,SAAA;MACA,KAAAJ,cAAA,CAAAG,kBAAA,CAAAC,SAAA;IACA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA,KAAAL,cAAA,CAAAM,QAAA,CAAAV,OAAA,QAAAA,OAAA;EACA;EACAW,KAAA;IACAX,OAAA,WAAAA,QAAAY,MAAA;MACA,KAAAR,cAAA,CAAAM,QAAA,CAAAV,OAAA,GAAAY,MAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}